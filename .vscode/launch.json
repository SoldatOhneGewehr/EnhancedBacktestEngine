{
  "version": "2.0.0",
  "configurations": [
    {
      "name": "Debug BacktestEngine",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/backtest",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },a
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build BacktestEngine",
      "miDebuggerPath": "/usr/bin/gdb"
    }
  ]
}
